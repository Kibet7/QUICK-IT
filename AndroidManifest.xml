<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    
    <EditText
        android:id="@+id/editTextText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="QUICK IT NOTES"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="6dp" />

    <Button
        android:id="@+id/FIRSTLEVEL"
        android:layout_width="wrap_content
        android:layout_height="wrap_content"
        android:text="FIRST LEVEL"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="144dp" />

    <EditText
        android:id="@+id/itnotes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Information Technology Notes"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="78dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" SECOND LEVEL"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="222dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="THIRD LEVEL"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="294dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FOURTH LEVEL"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="359dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
