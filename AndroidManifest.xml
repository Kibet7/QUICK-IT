<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="@string/quick_it_notes"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="6dp"
        android:autofillHints="QUICK"
        tools:ignore="LabelFor" />

    <EditText
        android:id="@+id/itNotes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="@string/information_technology_notes"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/editTextText"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="78dp"
        android:autofillHints="IT notes"
        tools:ignore="LabelFor" />

    <Button
        android:id="@+id/btnFirstLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/first_level"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/itNotes"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="144dp"
        tools:ignore="NotSibling" />

    <Button
        android:id="@+id/btnSecondLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/second_level"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnFirstLevel"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="222dp"
        tools:ignore="NotSibling" />

    <Button
        android:id="@+id/btnThirdLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/third_level"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnSecondLevel"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="294dp"
        tools:ignore="NotSibling" />

    <Button
        android:id="@+id/btnFourthLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fourth_level"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnThirdLevel"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="359dp"
        tools:ignore="NotSibling" />

</androidx.constraintlayout.widget.ConstraintLayout>
